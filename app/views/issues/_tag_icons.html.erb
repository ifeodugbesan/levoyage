<% case tag.name %>
<% when 'ruby' %>
  <%= image_tag 'ruby.svg' %>
<% when 'rails' %>
  <%= image_tag 'rails.svg' %>
<% when 'javascript' %>
  <%= image_tag 'javascript.svg' %>
<% when 'html' %>
  <%= image_tag 'html.svg' %>
<% when 'css' %>
  <%= image_tag 'css.svg' %>
<% when 'github' %>
  <%= image_tag 'github.svg' %>
<% when 'heroku' %>
  <%= image_tag 'heroku.svg' %>
<% end %>
